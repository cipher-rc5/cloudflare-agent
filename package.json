{
  "name": "cloudflare-agent",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "frontend:dev": "bun run --hot frontend/index.tsx",
    "frontend:build": "bun build frontend/index.tsx --outdir dist --target browser",
    "css:build": "bunx tailwindcss -i frontend/styles.css -o dist/styles.css --minify",
    "build": "bun run css:build && bun run frontend:build",
    "test": "vitest",
    "cf-typegen": "wrangler types"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.6.0",
    "@cloudflare/workers-types": "^4.20250109.0",
    "@types/bun": "^1.1.16",
    "typescript": "^5.7.3",
    "vitest": "2.1.8",
    "wrangler": "^3.101.0"
  },
  "dependencies": {
    "@envio-dev/hypersync-client": "^0.6.3",
    "@hono/zod-validator": "^0.4.2",
    "@reactflow/minimap": "^11.7.14",
    "@scalar/hono-api-reference": "^0.5.165",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@xyflow/react": "^12.3.6",
    "agent-twitter-client": "^0.0.18",
    "bun": "^1.1.43",
    "fxn-protocol-sdk": "https://github.com/Oz-Networks/fxn-protocol-sdk#main",
    "hono": "^4.6.16",
    "openai": "^4.78.0",
    "openapi3-ts": "^4.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reactflow": "^11.11.4",
    "tailwindcss": "^3.4.17",
    "viem": "^2.22.6"
  }
}
